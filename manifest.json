{
  "manifest_version": 2,

  "name": "Clear Service Worker",
  "description": "This extension easily un registers all the Service Workers loaded in chrome browser",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "24": "icon24.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "browsingData"
  ]
}
