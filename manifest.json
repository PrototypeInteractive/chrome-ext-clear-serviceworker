{
  "manifest_version": 2,

  "name": "Clear Service Worker",
  "description": "This extension easily unregisters Service Worker for the loaded page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "browsingData"
  ]
}
